{
	"domain": "news.exemple.net",
	"organization": "Nemoweb",
	"publicServer": ["news2.nemoweb.net"],
	"adminServer": ["news2.nemoweb.net"],
	"protocolVersion": "0.21.1",
	"administrator": "newsmaster@exemple.net",
	"maxDataLength": 1000,
	"crossDomainAccept": true,
	"activeInscription": true,
	"cryptPostingHost": "ifconnected",
	"activeLog": false,
	"logPath": "",
	"logFeedPath": "",
	"smtpHost": "smtp-relay.gmail.com",
	"smtpPort": 587,
	"smtpSecure": "TLS",
	"smtpAuth": false,
	"smtpLogin": "",
	"smtpPassword": "",
	"dbName": "nemoNews",
	"phpPath": "/usr/bin/php",
	"shellExec": true,
	"publicKey": "",
	"privateKey": "",
	"DataType": {
		"ProtoData": {
			"filter": ["Jid", "ID", "Data.DataType", "Data.DataID", "Data.InjectionDate", "Data.Protocol"]
		},
		"Article": {
			"filter": ["Data.Newsgroups", "Data.UserAgent", "Data.FromName", "Data.FromMail", "Data.Subject", "Data.ThreadID", "Data.UserID", "Data.ReferenceUserID", "Data.Control"]
		},
		"ListGroup": {
			"filter": ["Data.Newsgroups", "Data.Hierarchy"]
		},
		"Like": {
			"filter": ["Data.Newsgroups", "Data.JidLike"]
		}
	}
}
